{
    "name": "@plurid/deon",
    "version": "0.0.0-0",
    "description": "DeObject Notation Format of Structured Strings",
    "keywords": [
        "deon"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/deon"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/deon/issues"
    },
    "homepage": "https://github.com/plurid/deon/tree/master/packages/deon-javascript/README.md",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "bin": {
        "deon": "binder/deon"
    },
    "files": [
        "distribution",
        "binder"
    ],
    "preferGlobal": false,
    "scripts": {
        "test": "jest -c ./configurations/jest.config.js ./source",
        "test.suite": "jest -c ./configurations/jest.config.js",
        "watch": "rollup -c ./scripts/rollup.config.js -w",
        "clean": "rm -rf ./distribution",
        "build.cleanup": "find ./distribution -type d -name '*__tests__' -exec rm -rf {} + && find ./distribution -type f -name '*.DS_Store' -exec rm -rf {} +",
        "build.production": "rollup -c ./scripts/rollup.config.js",
        "build": "yarn clean && yarn build.production && yarn build.cleanup",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "commander": "^7.0.0",
        "cross-fetch": "^3.0.6",
        "encoding": "^0.1.13",
        "sync-fetch": "^0.3.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.1.0",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.27",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "eslint": "^7.19.0",
        "jest": "^26.6.3",
        "rollup": "^2.39.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-jest": "^26.5.1",
        "typescript": "^4.1.5"
    }
}

{
    #packages
    #package
    #commit
    yarnWorkspace false
    runFrom ../
}


packages [
    ./packages/deon-grammar/vscode
    ./packages/deon-javascript
]

package {
    manager yarn
    publisher npm
}

commit {
    engine git
    combine true
    root ''
    fullFolder false
    divider ' > '
    message setup: package
}
